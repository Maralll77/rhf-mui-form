{
  "name": "@paratco/mui-form",
  "version": "1.0.0",
  "description": "MUI + React Hook Form Reusable Form Components",
  "author": "Paratco",
  "license": "ISC",
  "repository": "git@github.com/Paratco/mui-form.git",
  "keywords": [
    "React",
    "RHF",
    "MUI",
    "Material UI",
    "React Hook Form",
    "Component"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "lint": "eslint . --report-unused-disable-directives-severity error --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@paratco/eslint-config": "^0.8.7",
    "@paratco/prettier-config": "^0.1.1",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.4"
  },
  "packageManager": "yarn@4.5.0"
}
